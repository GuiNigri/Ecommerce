@{ 
    Layout = "/Views/Shared/_Layout.cshtml";
}

<div class="section">
    <div class="container">
        <div class="row">
            <div class="col-md-12">

                <div>
                    <div class="row">
                        <div class="col-md-3">
                            <partial name="Manage/_ManageNav"/>
                        </div>
                        <div class="col-md-5 col-md-offset-2" id="layout-identity">
                            @RenderBody()
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>

@section Scripts {
    @RenderSection("Scripts", required: false)
}
